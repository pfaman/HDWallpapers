<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.hellosolver.hdwallpaper">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.SET_WALLPAPER" />
    <uses-permission
        android:name="android.permission.WRITE_EXTERNAL_STORAGE"
        tools:ignore="ScopedStorage" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/icons"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:roundIcon="@drawable/icon"
        android:supportsRtl="true"
        android:theme="@style/Theme.HDWALLPAPER">
        <activity
            android:name=".RakshabhandhanActivity"
            android:exported="false" />
        <activity android:name=".EmotionsActivity" />
        <activity android:name=".DiwaliActivity" />
        <activity android:name=".BikeActivity" />
        <activity android:name=".CarActivity" />
        <activity android:name=".TravelActivity" />
        <activity android:name=".VideoGameActivity" />
        <activity android:name=".BusinessActivity" />
        <activity android:name=".BuildingsActivity" />
        <activity android:name=".MusicActivity" />
        <activity android:name=".BackgroundsActivity" />
        <activity android:name=".TransportationActivity" />
        <activity android:name=".SportsActivity" />
        <activity android:name=".FoodActivity" />
        <activity android:name=".IndustryActivity" />
        <activity android:name=".AnimalsActivity" />
        <activity android:name=".PlacesActivity" />
        <activity android:name=".ComputerActivity" />
        <activity android:name=".ReligionActivity" />
        <activity android:name=".PeopleActivity" />
        <activity android:name=".HealthActivity" />
        <activity android:name=".EducationActivity" />
        <activity android:name=".ScienceActivity" />
        <activity android:name=".NatureActivity" />
        <activity android:name=".FashionActivity" />
        <activity
            android:name=".FullScreenActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".LandActivity"
            android:parentActivityName=".DashboardActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".SunriseActivity"
            android:parentActivityName=".DashboardActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".MountainActivity"
            android:parentActivityName=".DashboardActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".TreeActivity"
            android:parentActivityName=".DashboardActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".OceanActivity"
            android:parentActivityName=".DashboardActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".BeachActivity"
            android:parentActivityName=".DashboardActivity"
            android:screenOrientation="portrait" />
        <activity android:name=".DashboardActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>